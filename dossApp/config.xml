<?xml version='1.0' encoding='utf-8'?>
<widget id="fr.aurelienchampeval.appli" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>DailySong</name>
    <description>
        The principle is simple, every day you'll discover a new song and we hope, you'll like it.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Aurelien Champeval
    </author>
    <preference name="DisallowOverscroll" value="true" />
    <platform name="android">
        <icon src="www/img/logo.png" density="ldpi" />
        <icon src="www/img/logo.png" density="mdpi" />
        <icon src="www/img/logo.png" density="hdpi" />
        <icon src="www/img/logo.png" density="xhdpi" />
        <icon src="www/img/logo.png" density="xxhdpi" />
        <icon src="www/img/logo.png" density="xxxhdpi" />
    </platform>
    <content src="index.html" />
    <access origin="*" />
</widget>
